{
  "name": "@ctrl/plex",
  "version": "0.0.0-placeholder",
  "description": "plex api client in typescript",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": "typectrl/plex",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "plex",
    "typescript"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts, . --format codeframe",
    "lint:fix": "eslint --fix --ext .js,.ts, . --format codeframe",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest --runInBand --detectOpenHandles",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "claim-server": "ts-node test/claim-server.ts",
    "add-media": "ts-node test/add-media.ts",
    "test-cleanup": "ts-node test/test-cleanup.ts"
  },
  "dependencies": {
    "@ctrl/mac-address": "^2.1.0",
    "got": "^11.5.1",
    "p-any": "^3.0.0",
    "type-fest": "^0.16.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@ctrl/eslint-config": "1.0.4",
    "@types/lodash": "4.14.158",
    "@types/node": "14.0.27",
    "@types/puppeteer": "3.0.1",
    "@types/xml2js": "0.4.5",
    "@jest/globals": "26.2.0",
    "jest": "26.2.2",
    "jest-junit": "11.1.0",
    "lodash": "4.17.19",
    "puppeteer": "5.2.1",
    "ts-jest": "26.1.4",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=10"
  }
}

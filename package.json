{
  "name": "@ctrl/plex",
  "version": "0.0.0-placeholder",
  "description": "plex api client in typescript",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": "typectrl/plex",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "plex",
    "typescript"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "del dist && tsc -p tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --runInBand --watch",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "semantic-release": "semantic-release",
    "claim-server": "ts-node test/claim-server.ts",
    "add-media": "ts-node test/add-media.ts",
    "test-cleanup": "ts-node test/test-cleanup.ts"
  },
  "dependencies": {
    "@ctrl/mac-address": "^2.0.1",
    "got": "^11.0.3",
    "p-any": "^3.0.0",
    "type-fest": "^0.13.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/lodash": "4.14.150",
    "@types/node": "13.13.4",
    "@types/puppeteer": "^2.0.1",
    "@types/xml2js": "0.4.5",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "del-cli": "3.0.0",
    "eslint": "6.8.0",
    "eslint-config-xo-space": "0.24.0",
    "eslint-config-xo-typescript": "0.28.0",
    "eslint-plugin-import": "2.20.2",
    "jest": "25.5.3",
    "jest-junit": "10.0.0",
    "lodash": "4.17.15",
    "puppeteer": "3.0.2",
    "semantic-release": "17.0.7",
    "ts-jest": "25.4.0",
    "ts-node": "8.9.1",
    "typescript": "3.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=10"
  }
}
